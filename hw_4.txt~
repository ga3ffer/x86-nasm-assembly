ax = 0035		;move 0035 into ax
bl = ??			;move unknown value into bl
bx = ff??		;sign-extend bl into bx

    0035		;hex algebra
  + ff??
------------
    ff81

ax = ff81		;result of add bx to ax
eax = 0000 ff81		;zero-extend ax into eax

